<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.j2ee.mapper.FileMapper">
    <insert id="fileInsert">
        insert into file (user_id,file_address,file_name,file_from)
        values (#{user_id},#{file_address},#{file_name},#{file_from});
    </insert>

</mapper>